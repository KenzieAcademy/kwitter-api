{
  "name": "kwitter-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "dotenv-safe": "^6.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "openapi-enforcer": "^1.4.4",
    "openapi-enforcer-middleware": "^1.1.5",
    "openapi-enforcer-multer": "0.0.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^6.4.2",
    "sequelize": "^4.44.0",
    "sequelize-cli": "^4.1.1",
    "statuses": "^1.5.0",
    "swagger-ui-express": "^4.0.7",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "0.0.9-4.0.0-beta3",
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "start": "node index.js",
    "watch": "nodemon",
    "postinstall": "npm run migrate:up",
    "migrate:up": "if [ -e .env ]; then . ./.env; fi && sequelize db:migrate --url $DATABASE_URL",
    "migrate:down": "if [ -e .env ]; then . ./.env; fi && sequelize db:migrate:undo --url $DATABASE_URL"
  }
}
